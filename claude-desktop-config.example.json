{
  "mcpServers": {
    "espn-fantasy-manager": {
      "command": "node", 
      "args": ["/path/to/your/project/fantasy-poc/mcp-server/dist/index.js"],
      "env": {
        "ESPN_S2": "your_espn_s2_cookie_here",
        "ESPN_SWID": "your_espn_swid_cookie_here",
        "LEAGUE_1_ID": "your_league_1_id",
        "LEAGUE_1_TEAM_ID": "your_team_id",
        "LEAGUE_1_NAME": "Your League Name",
        "LEAGUE_2_ID": "your_league_2_id", 
        "LEAGUE_2_TEAM_ID": "your_team_id",
        "LEAGUE_2_NAME": "Your Other League Name",
        "GEMINI_API_KEY": "your_gemini_api_key_here",
        "DEFAULT_LLM_PROVIDER": "gemini",
        "FANTASYPROS_SESSION_ID": "your_fantasypros_session_id_here",
        "DAILY_COST_LIMIT": "2.00",
        "WEEKLY_COST_LIMIT": "10.00", 
        "MONTHLY_COST_LIMIT": "35.00"
      }
    }
  }
}